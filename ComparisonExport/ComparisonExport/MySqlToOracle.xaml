        <TextBlock Height="23" HorizontalAlignment="Left" Margin="282,67,0,0" Name="textBlock3" Text="地址：" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="324,67,0,0" Name="txtAddrTo" VerticalAlignment="Top" Width="146" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="282,101,0,0" Name="textBlock4" Text="端口：" VerticalAlignment="Top" />
